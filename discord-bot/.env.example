# Discord Bot Token (same as used for interactions)
DISCORD_BOT_TOKEN=your_discord_bot_token_here

# Supabase Edge Function URL for syncing messages
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_FUNCTIONS_URL=https://your-project.supabase.co/functions/v1

# Secret for authenticating with edge functions
# This should be set to the same value as DISCORD_BOT_SECRET in your Supabase secrets
DISCORD_BOT_SECRET=your_shared_secret_here

# Optional: Log level (debug, info, warn, error)
LOG_LEVEL=info
