{
  "buildCommand": "next build",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/auth",
      "destination": "https://cr-auth.vercel.app/auth"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/sw.js",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ]
}
